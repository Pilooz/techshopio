  <link href="<%= APP_PATH %>/app/vendor/bootstrap/dist/css/bootstrap.min.css?v=<%=APP_VERSION%>" rel="stylesheet" />
  <link href="<%= APP_PATH %>/app/vendor/Font-Awesome/css/font-awesome.min.css?v=<%=APP_VERSION%>" rel="stylesheet" />
